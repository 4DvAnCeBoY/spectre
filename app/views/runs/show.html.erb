<ul class="breadcrumb">
  <li><%= link_to 'Projects', projects_url %></li>
  <li><%= link_to @run.suite.project.name, projects_url %></li>
  <li><%= link_to @run.suite.name, suite_url(@run.suite) %></li>
  <li>#<%= @run.sequential_id %></li>
</ul>

<h1>Run #<%= @run.sequential_id %> <span class="text-muted">(<%= @run.suite.name %>)</span></h1>

<%= render 'shared/test_filters', tests: @tests %>

<table class="table table-striped ">
  <thead class="thead-inverse">
    <tr>
      <th>Test</th>
      <th>Baseline</th>
      <th>Compare</th>
      <th>Diff</th>
      <th>Result</th>
    </tr>
  </thead>
  <tbody>
    <% @tests.tests.each do |test| %>
      <%= render test %>
    <% end %>
  </tbody>
</table>
